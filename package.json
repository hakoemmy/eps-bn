{
  "name": "E-procurement System",
  "version": "1.0.0",
  "description": "The NodeJs/Express application server for E-procurement System",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src",
    "build": "babel src -d build",
    "start": "node build",
    "eslint": "npx eslint --ignore-path .gitignore . --fix",
    "prettier": "npx prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\" --config .prettierrc --ignore-path .gitignore",
    "pretest": "npm run migrate:undo && npm run seed",
    "test": "npx jest --coverage --detectOpenHandles",
    "migrate:undo": "npx sequelize-cli db:migrate:undo:all",
    "postmigrate:undo": "npm run migrate",
    "migrate": "npx sequelize-cli db:migrate",
    "preseed": "npx sequelize-cli db:seed:undo:all",
    "seed-dev": "cross-env NODE_ENV=development npx sequelize-cli db:seed --seed 20200529085617-role-seed && cross-env NODE_ENV=development npx sequelize-cli db:seed --seed 20200529085618-users-seed"
  },
  "keywords": [
    "nodejs",
    "express",
    "sequelize",
    "postgreSQL"
  ],
  "author": "Emmanuel Hakorimana",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.12.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@sendgrid/mail": "^7.4.2",
    "accesscontrol": "^2.2.1",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.10.0",
    "clickatell-platform": "^2.0.11",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "downloadjs": "^1.4.7",
    "export-to-csv": "^0.2.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "firebase": "^8.2.1",
    "firebase-admin": "^9.4.2",
    "google-auth-library": "^6.1.4",
    "helmet": "^4.4.1",
    "http-status-codes": "^2.1.4",
    "imap": "^0.8.19",
    "imap-simple": "^5.0.0",
    "joi": "^17.3.0",
    "joi-phone-number": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "mailparser": "^3.1.0",
    "mod10-check-digit": "^1.0.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-microsoft": "^0.1.0",
    "pdf-creator-node": "^1.4.2",
    "pdf-to-excel": "^0.3.0",
    "pdf2table": "0.0.2",
    "pdfreader": "^1.2.8",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "pg-native": "^3.0.0",
    "read-excel-file": "^5.0.0",
    "sequelize": "^6.3.5",
    "uuid": "^8.3.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "private": false,
  "devDependencies": {
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1"
  }
}
